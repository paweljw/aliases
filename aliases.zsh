# Aliases

alias are="source ~/.aliases/bootstrap.zsh"
alias apu='cd ~/.aliases && git add . && git commit -m "Added aliases @ `date`" && git push origin master; cd -'
alias aed='nvim ~/.aliases'
alias au='cd ~/.aliases && git pull && cd -'

alias yas='yard server'
alias yad='yard doc'
alias chv='H2_VM_RAM_SIZE=3072 vagrant'
